{
  "id": "set_speed_limit",
  "title": {
    "en": "Set speed limit"
  },
  "args": [
    {
      "name": "device",
      "type": "device",
      "filter": "driver_id=Tesla"
    },
    {
      "name": "limit_kmh",
      "type": "range",
      "min": 81,
      "max": 144,
      "step": 1,
      "label": " km/h",
      "labelDecimals": 0,
      "title": {
        "en": "Speed limit (km/h)"
      }
    },
    {
      "name": "state",
      "type": "dropdown",
      "values": [
        {
          "id": "OFF",
          "label": {
            "en": "Deactivate"
          }
        },
        {
          "id": "ON",
          "label": {
            "en": "Activate"
          }
        }
      ]
    },
    {
      "name": "pin",
      "type": "number",
      "min": 1,
      "max": 1000,
      "step": 1,
      "title": {
        "en": "Pin"
      },
      "placeholder": {
        "en": "Pin..."
      }
    }
  ]
}
